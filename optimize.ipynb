    "        for (sea_fuel_cost, steaming_days) in self.sea_fuel_costs():\n",
    "            if sea_fuel_cost == np.inf: continue\n",
    "            total_days = steaming_days + self.cargo.loadport_days() + self.cargo.disport_days()\n",
